#include<stdio.h>
#include<math.h>
int main()
{
    while(1)
    {
        int n;
        printf("Any integer Value: ");
        scanf("%d",&n);
        int firstNum=2;
        int secondNum=1;
        int ansMain;
        for(int i =2;i<=n;i++)
        {
            ansMain = firstNum+secondNum;
            firstNum=secondNum;
            secondNum=ansMain;
        }
        printf("The Lucus of %d is: %d\n",n,ansMain);

    }

}


