#include<bits/stdc++.h>
using namespace std;
int main()
{
    int num;
    cin>>num;
    while(num>9)
    {

        int rem;
        int sum =0;
        while(num!=0)
        {
            rem=num%10;
            num=num/10;
            sum=sum+rem;
        }
        cout<<sum<<endl;
        num=sum;
    }

}
