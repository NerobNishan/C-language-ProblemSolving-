
#include<stdio.h>
#include<ctype.h>
#include<math.h>
int main()
{
    while(1)
    {
        int num;
        printf("Give any integer Number: ");
        scanf("%d",&num);

        int temp=num; //As orginal number don't change
        int Sum=0;

        while(temp!=0)
        {
            int reminder=temp%10;
            Sum=Sum+reminder*reminder*reminder;
            temp=temp/10;
        }
        printf("The Sum for Armstrong: %d\n",Sum);
        if(Sum==num)
            printf("%d is a Armstrong\n",num);
        else
            printf("%d is not a Armstrong\n",num);
    }
}
