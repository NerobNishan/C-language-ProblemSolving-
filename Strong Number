#include<stdio.h>
#include<math.h>
int main()
{
    while(1)
    {
        int num,rem;
        printf("Give any Integer Value: ");
        scanf("%d",&num);

        int sum=0;
        int n1=num;
        while(n1!=0)
        {
            int fact=1;
            rem=n1%10;
            for(int i=1;i<=rem;i++)
            {
                fact=fact*i;
            }
            sum=sum+fact;
            n1=n1/10;
        }
        if(num==sum)
            printf("%d is a Strong Number\n",num);
        else
            printf("%d is not a Strong Number\n",num);
    }
}
