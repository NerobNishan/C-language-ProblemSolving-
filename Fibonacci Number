#include<stdio.h>
#include<math.h>
int main()
{
    while(1)
    {
        int n;
        printf("Any integer Value: ");
        scanf("%d",&n);
        int firstNum=0;
        int secondNum=1;
        int ansMain;
        for(int i =2;i<=n;i++)
        {
            ansMain = firstNum+secondNum;
            firstNum=secondNum;
            secondNum=ansMain;
        }
        printf("The fibonacci of %d is: %d\n",n,ansMain);

    }
//int n=8;
//double sq5=sqrt(5);
//double f1=(pow((1+sq5),n)-pow((1-sq5),n));
//double f2=(pow(2,n)*sq5);
//double fibo=f1/f2;
//
//printf("%.0f",fibo);

}


