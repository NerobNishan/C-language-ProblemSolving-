#include<bits/stdc++.h>
using namespace std;

int partition (int arr[],int start,int end){

    int pos = start;
    for(int i=start; i<=end; i++){
        if(arr[i]<=arr[end]){
            swap(arr[i],arr[pos]);
            pos++;
        }
    }

    return pos-1;
}

void quickSort(int arr[],int start,int end){
    if(start>=end)
        return;

    int pivot = partition(arr,start,end);

    quickSort(arr,start,pivot-1);
    quickSort(arr,pivot+1,end);

}

int main(){
    int n;
    cout<<"The Number of Value: ";
    cin>>n;

    int arr[n];
    cout<<"Give the Value:"<<endl;
    for(int i=0;i<n;i++){
        cin>>arr[i];
    }

    quickSort(arr,0,n-1);
    cout<<"Sorted Array: "<<endl;
    for(int i=0;i<n;i++){
        cout<<arr[i]<<" ";
    }
    cout<<endl;
    return 0;
}
